{
  "name": "react-todo",
  "version": "1.0.0",
  "description": "React TODO App",
  "main": "index.js",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "start": "node -r babel-register server.js",
    "browser-sync": "browser-sync start --config bs-config.js --color",
    "build": "webpack",
    "watch": "concurrently \"webpack --watch --color\" \"npm run browser-sync\"",
    "pretest": "npm run lint",
    "test": "mocha --compilers jsx:babel-core/register test",
    "test:e2e": "wdio wdio.conf.js",
    "lint": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "postinstall": "if [ $BUILD_ASSETS ]; then npm run build -- -p; fi"
  },
  "keywords": [],
  "author": "ybiquitous <10koba01@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-register": "^6.14.0",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.13.0",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.5",
    "wdio-mocha-framework": "^0.4.0",
    "webdriver-manager": "^10.2.3",
    "webdriverio": "^4.2.8",
    "webpack": "^1.13.2"
  }
}
