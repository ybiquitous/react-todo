{
  "name": "react-todo",
  "version": "1.0.0",
  "description": "React TODO App",
  "main": "server.js",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "start": "DEBUG=app node -r babel-register server.js",
    "browser-sync": "browser-sync start --config bs-config.js --color",
    "build": "webpack",
    "watch": "concurrently \"webpack --watch --color\" \"npm run browser-sync\"",
    "pretest": "npm run eslint:all && npm run stylelint:all",
    "test": "mocha --reporter dot --compilers jsx:babel-register test",
    "test:e2e": "wdio wdio.conf.js",
    "eslint:all": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "stylelint:all": "stylelint --ignore-path .gitignore \"./src/styles/**/*\"",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "postinstall": "if [ $BUILD_ASSETS ]; then npm run build -- -p; fi",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.@(js|jsx)": "eslint",
    "*.@(css|scss)": "stylelint"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "keywords": [],
  "author": "ybiquitous <10koba01@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-register": "^6.14.0",
    "debug": "^2.2.0",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.13.0",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "lint-staged": "^3.0.1",
    "mocha": "^3.0.2",
    "postcss-loader": "^0.13.0",
    "pre-commit": "^1.1.3",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "wdio-mocha-framework": "^0.4.0",
    "webdriver-manager": "^10.2.3",
    "webdriverio": "^4.2.8",
    "webpack": "^1.13.2",
    "webpack-manifest-plugin": "^1.0.1"
  }
}
